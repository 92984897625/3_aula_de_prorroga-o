programa {
  inclua biblioteca Util --> u
  funcao inicio() {
   inteiro numero, seu_numero
   numero = u.sorteia(1,100)
    
    faca{ 
      escreva ("adivinhe um número entre 1 e 100: ")
      leia (seu_numero)
      se (seu_numero != numero) {
       se (seu_numero < numero) {
        escreva (seu_numero, " é menor que o \"numero\" ¯\\_(ツ)_/¯")
      }
      }
    }enquanto(numero != seu_numero)
  }
}
